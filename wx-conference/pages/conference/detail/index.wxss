.upload-img-container {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-items: center;
}

.upload-img-container .chooseView {
  width: 190rpx;
  height: 166rpx;
  background-color: #fff;
  margin-right: 40rpx;
  border: 2rpx solid lightgray;
  text-align: center;
  line-height: 180rpx;
}

.upload-img-container .chooseImv {
  width: 50rpx;
  height: 50rpx;
}

.upload-img-container .upload-img {
  margin-right: 40rpx;
  margin-bottom: 40rpx;
  width: 190rpx;
  height: 166rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  position: relative;
}

.upload-img-container .upload-img .img-item {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.upload-img-container .upload-img .closeImv {
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  width: 50rpx;
  height: 50rpx;
}

